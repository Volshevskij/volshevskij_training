<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  
  <system.serviceModel>
    <services>
      <service name="WCFServiceLib.PhotoService">
        <endpoint address="WCFServiceLib.PhotoService" binding="basicHttpBinding" contract="WCFServiceLib.IPhotoService"></endpoint>
        <endpoint address="WCFServiceLib.PhotoService" binding="netTcpBinding" contract="WCFServiceLib.IPhotoService"></endpoint>
        <endpoint address="mex" binding="mexHttpsBinding" contract ="IMetadataExchange"></endpoint>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8080/"/>
            <add baseAddress="net.tcp://localhost:45617/"/>
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="mexBehavior">
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>